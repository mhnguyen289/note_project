<h4>Here are your current Notebooks today:  </h4>
    <div class="wrapper_with_padding">
        <div class="clearfix" id="notes">
         <% @notebooks.collect do |notebook| %>
            <div class="note">
                <p class="title">    
    	           <%= link_to notebook.title, notebook_path(@notebook) %>
    	    	</p>	
            </div>
    	<%end%>
 	</div>
</div>